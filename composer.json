{
    "name": "abexto/yii2-yepa-phpunit",
    "description": "PhpUnit Helper Class for Yii2 Projects",
    "type": "yii2-extension",
    "keywords": ["yii", "yii2", "tests", "PhpUnit"],
    "authors": [
        {
            "name": "Andreas Prucha, Abexto - Helicon Software Development",
            "email": "prucha@helicon.co.at"
        }
    ],
    "require": {
        "yiisoft/yii2": "*"
    },
    "autoload": {
        "psr-4": {
            "abexto\\yepa\\phpunit\\": "lib"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "abexto\\yepa\\phpunit\\tests\\": "tests"
        }
    }
}
